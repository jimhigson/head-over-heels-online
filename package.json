{
  "name": "hohjs",
  "private": true,
  "version": "1.18.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jimhigson/head-over-heels-online"
  },
  "homepage": "https://blockstack.ing",
  "type": "module",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev:game": "node --max_http_header_size=128000 node_modules/vite/bin/vite",
    "dev:editor": "vite --config vite.editor.config.ts --mode development",
    "preview:game": "vite preview",
    "preview:editor": "vite --config vite.editor.config.ts preview",
    "build:game": "vite build",
    "build:editor": "vite build --config vite.editor.config.ts --emptyOutDir",
    "build": "pnpm run \"/^build:.*/\"",
    "//check:build": "I don't know any way to check the build will pass other than running it:",
    "check:build:game": "pnpm build:game",
    "check:build:editor": "pnpm build:editor",
    "watch:type": "tsc --noEmit --watch --project ./tsconfig.app.json",
    "check:type": "tsc --noEmit --project ./tsconfig.app.json",
    "check:lint": "eslint .",
    "check": "COLOR=1 pnpm run \"/^check:.*/\"",
    "fix": "COLOR=1 pnpm run \"/^fix:.*/\"",
    "fix:lint": "eslint --fix .",
    "check:format": "prettier . --check",
    "fix:format": "prettier . --write",
    "check:test": "vitest run",
    "watch:test": "vitest watch",
    "bench": "pnpm vitest bench",
    "build-and-playwright": "pnpm build:game && playwright test",
    "json": "tsx src/campaignXml2Json/scripts/xml2json.ts && prettier src/_generated/originalCampaign --write",
    "jsonpatch": "tsx src/campaignXml2Json/scripts/makeCampaignDiff.ts && prettier src/_generated/originalCampaign/patches/*.json --write",
    "predeploy": "pnpm run check && pnpm run build && echo blockstack.ing > dist/CNAME && cp dist/index.html dist/404.html",
    "deploy": "gh-pages -d dist",
    "iff2png": "./scripts/iff2png.sh",
    "watch:iff2png": "pnpm run iff2png && chokidar gfx/sprites.iff -c \"pnpm run iff2png\"",
    "check:circDep": "dpdm -T --exit-code circular:1 src/main.tsx",
    "gen:types": "./scripts/itemTypesFromTs.ts src/model/ItemInPlay.ts ItemInPlayType > src/_generated/types/ItemInPlayUnion.ts && prettier src/_generated/types/ --write",
    "gen:roomSchema:ts": "./scripts/schema/flattenType.ts --file src/model/RoomJson.ts --type AnyRoomJson --output-type RoomJsonSchema | prettier --parser typescript > src/_generated/Room.flat.d.ts",
    "gen:roomSchema:json": "ts-json-schema-generator --path src/_generated/Room.flat.d.ts --type RoomJsonSchema | scripts/schema/postProcessSchema.ts | prettier --parser json > src/_generated/room.schema.json",
    "gen:roomSchema": "pnpm gen:roomSchema:ts; pnpm gen:roomSchema:json",
    "gen:dbTypes": "supabase gen types typescript --project-id pkswdnpftrundnewgnya > src/_generated/db.d.ts; prettier --write src/_generated/db.d.ts",
    "dumpDb:schema": "pg_dump-17 -h aws-0-eu-west-2.pooler.supabase.com -p 6543 -d postgres -U postgres.pkswdnpftrundnewgnya --schema-only --schema=public --no-owner --no-privileges > db/schema.sql",
    "dumpDb:data": "pg_dump-17 -h aws-0-eu-west-2.pooler.supabase.com -p 6543 -d postgres -U postgres.pkswdnpftrundnewgnya --data-only --schema=public --no-owner --no-privileges > db/data.sql",
    "prepare": "husky"
  },
  "dependencies": {
    "@blockstacking/jims-shaders": "^0.4.3",
    "@github/mini-throttle": "^2.1.1",
    "@monaco-editor/react": "^4.7.0",
    "@mutabazia/react-portal": "^1.0.7",
    "@octokit/openapi-types": "^27.0.0",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-popover": "^1.1.6",
    "@reduxjs/toolkit": "^2.6.1",
    "@tauri-apps/plugin-global-shortcut": "~2.3.1",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@types/nano-equal": "^2.0.2",
    "@types/natural-compare-lite": "^1.4.2",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "es-iterator-helpers": "^1.2.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "husky": "^9.1.7",
    "iter-tools-es": "^7.5.4",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.52.2",
    "nano-equal": "^2.0.2",
    "natural-compare-lite": "^1.4.0",
    "natural-orderby": "^5.0.0",
    "pixi.js": "8.16.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^3.0.3",
    "redux-persist": "^6.0.0",
    "snarkdown-in-react": "^2.0.3",
    "string-transform": "^21.9.19",
    "tailwind-merge": "^2.6.0",
    "tailwind-scrollbar": "^3.1.0",
    "timeago.js": "^4.0.2",
    "use-immer": "^0.11.0",
    "wouter": "^3.9.0"
  },
  "devDependencies": {
    "@dyljhd/use-effect-debugger": "^1.2.1",
    "@eslint/js": "^9.22.0",
    "@playwright/test": "^1.57.0",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.93.1",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/cli": "^2.9.6",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/seedrandom": "^3.0.8",
    "@types/umami": "^2.10.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "ajv": "^8.17.1",
    "chalk": "^5.4.1",
    "chokidar-cli": "^3.0.0",
    "commander": "^14.0.0",
    "dougmencken/HeadOverHeels": "github:dougmencken/HeadOverHeels#08aee0cb050405dc23d95bf288631452bec28c9f",
    "dpdm": "^3.14.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fast-json-patch": "^3.1.1",
    "gh-pages": "^6.3.0",
    "git-format-staged": "^3.1.1",
    "globals": "^15.15.0",
    "hohjs-production": "github:jimhigson/head-over-heels-online#production",
    "image-size": "^1.2.0",
    "immer": "^10.1.1",
    "js-base64": "^3.7.7",
    "json-canonicalize": "^1.0.6",
    "jsonc-parser": "^3.3.1",
    "kill-port": "^2.0.1",
    "knip": "^5.63.0",
    "normalize-wheel-es": "^1.2.0",
    "pixi-filters": "^6.1.5",
    "postcss": "^8.5.3",
    "prettier": "3.4.2",
    "react-resize-detector": "^12.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "seedrandom": "^3.0.5",
    "supabase": "^2.72.8",
    "tailwindcss": "^3.4.17",
    "ts-json-schema-generator": "^2.4.0",
    "ts-morph": "^25.0.1",
    "tsx": "^4.19.3",
    "type-fest": "^4.37.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.3.1",
    "vite-plugin-glsl": "^1.5.5",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17",
    "workbox-window": "^7.3.0",
    "xml-js": "^1.6.11"
  },
  "engines": {
    "node": ">=22.21.1"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "supabase"
    ]
  }
}