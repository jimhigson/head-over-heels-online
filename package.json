{
  "name": "hohjs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "GIT_HASH=\"$(git rev-parse --short HEAD)\" BUILD_DATE=\"$(date +%Y-%m-%d)\" vite",
    "build": "GIT_HASH=\"$(git rev-parse --short HEAD)\" BUILD_DATE=\"$(date +%Y-%m-%d)\" vite build",
    "//check:build": "I don't know any way to check the build will pass other than running it:",
    "check:build": "pnpm build",
    "watch:type": "tsc --noEmit --watch --project ./tsconfig.app.json",
    "check:type": "tsc --noEmit --project ./tsconfig.app.json",
    "check:lint": "eslint .",
    "check": "pnpm run \"/^check:.*/\"",
    "fix:lint": "eslint --fix .",
    "preview": "vite preview",
    "check:format": "prettier . --check",
    "fix:format": "prettier . --write",
    "check:test": "vitest run",
    "watch:test": "vitest watch",
    "json": "tsx src/campaignXml2Json/scripts/xml2json.ts && prettier src/_generated/originalCampaign/rooms --write",
    "jsonpatch": "tsx src/campaignXml2Json/scripts/makeCampaignDiff.ts && prettier src/_generated/originalCampaign/patches/*.json --write",
    "predeploy": "pnpm run check && pnpm run build && echo blockstack.ing > dist/CNAME && cp dist/index.html dist/404.html",
    "deploy": "gh-pages -d dist",
    "iff2png": "./scripts/iff2png.sh",
    "watch:iff2png": "pnpm run iff2png && chokidar gfx/sprites.iff -c \"pnpm run iff2png\"",
    "check:circDep": "dpdm -T --exit-code circular:1 src/main.tsx",
    "genTypes": "./scripts/itemTypesFromTs.ts src/model/ItemInPlay.ts ItemInPlayType > src/_generated/types/ItemInPlayUnion.ts && prettier src/_generated/types/ --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@mutabazia/react-portal": "^1.0.7",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-popover": "^1.1.6",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/nano-equal": "^2.0.2",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "es-iterator-helpers": "^1.2.1",
    "husky": "^9.1.7",
    "nano-equal": "^2.0.2",
    "natural-orderby": "^5.0.0",
    "pixi.js": "^8.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "snarkdown-in-react": "^2.0.2",
    "tailwind-merge": "^2.6.0",
    "tailwind-scrollbar": "^3.1.0",
    "ts-morph": "^25.0.1",
    "wouter": "^3.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/seedrandom": "^3.0.8",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.4.1",
    "chokidar-cli": "^3.0.0",
    "dpdm": "^3.14.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fast-json-patch": "^3.1.1",
    "gh-pages": "^6.3.0",
    "git-format-staged": "^3.1.1",
    "globals": "^15.15.0",
    "image-size": "^1.2.0",
    "immer": "^10.1.1",
    "iter-tools": "^7.5.3",
    "json-canonicalize": "^1.0.6",
    "pixi-filters": "^6.1.0",
    "postcss": "^8.5.3",
    "prettier": "3.4.2",
    "react-resize-detector": "^12.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "seedrandom": "^3.0.5",
    "shorthash2": "^1.0.5",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "type-fest": "^4.37.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.0.8",
    "workbox-window": "^7.3.0",
    "xml-js": "^1.6.11"
  },
  "engines": {
    "node": ">=23.11.0"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  }
}