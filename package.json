{
  "name": "hohjs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch:type": "tsc --noEmit --watch --project ./tsconfig.app.json",
    "check:type": "tsc --noEmit --project ./tsconfig.app.json",
    "check:lint": "eslint .",
    "check:all": "pnpm run check:lint && pnpm run check:type && pnpm run check:format && pnpm run test",
    "fix:lint": "eslint --fix .",
    "preview": "vite preview",
    "check:format": "prettier --experimental-ternaries . --check",
    "fix:format": "prettier --experimental-ternaries . --write",
    "test": "vitest",
    "worldJson": "tsx scripts/campaignXml2Json/convertCampaign.ts && prettier src/_generated/originalCampaign --write",
    "makeWorldJsonPatch": "tsx scripts/campaignXml2Json/makeCampaignDiff.ts > src/_generated/originalCampaign/patch.json && prettier src/_generated/originalCampaign/patch.json --write",
    "predeploy": "pnpm run check:lint && pnpm run check:type && pnpm run check:format && pnpm run build",
    "deploy": "gh-pages -d dist",
    "sprites2png": "./scripts/sprites2png.sh",
    "watch:sprites2png": "pnpm run sprites2png && chokidar gfx/sprites.iff -c \"pnpm run sprites2png\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "lucide-react": "^0.453.0",
    "pixi.js": "^8.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/toposort": "^2.0.7",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.3.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fast-json-patch": "^3.1.1",
    "gh-pages": "^6.2.0",
    "globals": "^15.9.0",
    "immer": "^10.1.1",
    "iter-tools": "^7.5.3",
    "json-canonicalize": "^1.0.6",
    "mitt": "^3.0.1",
    "pixi-filters": "^6.0.5",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "shorthash2": "^1.0.5",
    "tailwindcss": "^3.4.13",
    "toposort": "^2.0.2",
    "tsx": "^4.19.1",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.4",
    "xml-js": "^1.6.11"
  },
  "volta": {
    "node": "22.9.0"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
}
