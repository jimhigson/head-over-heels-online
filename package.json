{
  "name": "hohjs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch:type": "tsc --noEmit --watch --project ./tsconfig.app.json",
    "check:type": "tsc --noEmit --project ./tsconfig.app.json",
    "check:lint": "eslint .",
    "check": "pnpm run \"/^check:.*/\"",
    "fix:lint": "eslint --fix .",
    "preview": "vite preview",
    "check:format": "prettier --experimental-ternaries . --check",
    "fix:format": "prettier --experimental-ternaries . --write",
    "check:test": "vitest run",
    "watch:test": "vitest watch",
    "json": "tsx src/campaignXml2Json/convertCampaign.ts && prettier src/_generated/originalCampaign --write",
    "jsonpatch": "tsx src/campaignXml2Json/makeCampaignDiff.ts > src/_generated/originalCampaign/patch.json && prettier src/_generated/originalCampaign/patch.json --write",
    "predeploy": "pnpm run check && pnpm run build && echo blockstack.ing > dist/CNAME",
    "deploy": "gh-pages -d dist",
    "iff2png": "./scripts/iff2png.sh",
    "watch:iff2png": "pnpm run iff2png && chokidar gfx/sprites.iff -c \"pnpm run iff2png\"",
    "check:circDep": "dpdm -T --exit-code circular:1 src/main.tsx"
  },
  "dependencies": {
    "@mutabazia/react-portal": "^1.0.7",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-switch": "^1.1.2",
    "@reduxjs/toolkit": "^2.5.0",
    "@types/nano-equal": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "es-iterator-helpers": "^1.2.1",
    "nano-equal": "^2.0.2",
    "natural-orderby": "^5.0.0",
    "pixi.js": "^8.6.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "snarkdown-in-react": "^2.0.1",
    "tailwind-merge": "^2.6.0",
    "tailwind-scrollbar": "^3.1.0",
    "wouter": "^3.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.4.1",
    "chokidar-cli": "^3.0.0",
    "dpdm": "^3.14.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unused-imports": "^4.1.4",
    "fast-json-patch": "^3.1.1",
    "gh-pages": "^6.2.0",
    "globals": "^15.14.0",
    "image-size": "^1.2.0",
    "immer": "^10.1.1",
    "iter-tools": "^7.5.3",
    "json-canonicalize": "^1.0.6",
    "mitt": "^3.0.1",
    "pixi-filters": "^6.0.5",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "shorthash2": "^1.0.5",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "type-fest": "^4.30.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^2.1.8",
    "xml-js": "^1.6.11"
  },
  "volta": {
    "node": "22.9.0"
  },
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
}